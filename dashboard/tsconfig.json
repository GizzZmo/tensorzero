{
    "compilerOptions": {
      "target": "es2017",
      "lib": ["es2017", "dom"],
      "module": "commonjs",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "types": ["node"],
      "jsx": "react"
    },
    "include": ["**/*.ts", "**/*.tsx"],

    "exclude": ["node_modules"]
  }
